<%- include("header"); -%>
    <div id="books-cont">
        <% for (let i=0; i < bookdata.length; i++) { %>
            <div class="book-card">
                <h2>
                    <%= bookdata[i].title %>
                </h2>
                <p>
                    <%= bookdata[i].description %>
                </p>
                <p>Pages: <%= bookdata[i].pages %>
                </p>
                <a href=<%=bookdata[i].bookLink %>>Drive Link</a>
            </div>
            <% } %>
    </div>
    <%- include("footer"); -%>